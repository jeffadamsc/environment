[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    history = log --pretty="tformat:'%h %ad | %s%d [%an]'" --graph --date=short
    hs = log --pretty="tformat:'%h %ad | %s%d [%an]'" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[push]
	default = simple
[branch]
	autosetuprebase = always
